//jsp模板
   html代码就是jsp模板


//jsp表达式
  import="java.util.*"
    <% String name="kelisi" %>
    <%=name %>
 
 
// jsp脚本：for循环
    <% 
         for(int i=0;i<10;i++){ 
    %>
       <h3>输出10次</h3>
    <% } %>


//jsp声明   
  <%!
     String email="258@qq.com";
     public void run(){
     System.out.println("跑呀跑");
     }
  %>


//jsp注释
  <%-- --%>


//Jsp三大指令
//语法<%@ %>
// 1.taglib


// 2.include
   用于包含其他页面

//  3.page
